<h3 style="color: blueviolet;"><a href="/exportdata">Export the data to csv</a></h3>
<table class="container body-content">
  <thead>
    <tr>
      <th>Application ID</th>
      <th>Name</th>
      <th>Age</th>
      <th>DOB</th>
    </tr>
  </thead>
  <tbody>
    <%if (data.length >0){ %>
    <% data.forEach(function(row){ %>
    <tr>
      <td><%= row.app_id %></td>
      <td><%= row.name %></td>
      <td><%= row.age %></td>
      <td><%= row.dob %></td>
    </tr>
    <% })}else{ %>
      <tr>
          <td colspan="6">No Record Found</td>
        </tr>
      <% } %>
  </tbody>
</table>